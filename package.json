{
  "name": "@nuxthub/core",
  "private": false,
  "version": "0.8.18",
  "description": "Build full-stack Nuxt applications on Cloudflare, with zero configuration.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-hub/core.git"
  },
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "homepage": "https://hub.nuxt.com",
  "files": [
    "dist"
  ],
  "keywords": [
    "nuxt",
    "fullstack",
    "cloudflare",
    "database",
    "kv",
    "blob"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub; nuxt-module-build prepare; nuxi prepare playground",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "docs": "PORT=4000 nuxi dev docs",
    "docs:build": "nuxi generate docs",
    "release": "npm run lint && npm run test && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --test-timeout=60000",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250303.0",
    "@nuxt/devtools-kit": "^2.1.3",
    "@nuxt/kit": "^3.15.4",
    "@uploadthing/mime-types": "^0.3.4",
    "citty": "^0.1.6",
    "confbox": "^0.2.1",
    "defu": "^6.1.4",
    "destr": "^2.0.3",
    "h3": "^1.15.1",
    "mime": "^4.0.6",
    "nitro-cloudflare-dev": "^0.2.1",
    "ofetch": "^1.4.1",
    "pathe": "^2.0.3",
    "pkg-types": "^2.1.0",
    "std-env": "^3.8.1",
    "ufo": "^1.5.4",
    "uncrypto": "^0.1.3",
    "unstorage": "^1.15.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.1.3",
    "@nuxt/eslint-config": "^1.1.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.17.1",
    "@nuxthub/core": "link:",
    "@types/node": "^22.13.9",
    "changelogen": "^0.6.1",
    "eslint": "^9.21.0",
    "nuxt": "^3.15.4",
    "typescript": "5.6.3",
    "vitest": "^3.0.7",
    "wrangler": "^3.112.0"
  },
  "resolutions": {
    "h3": "^1.14.0"
  },
  "packageManager": "pnpm@10.5.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tsparticles/engine",
      "better-sqlite3",
      "esbuild",
      "puppeteer",
      "sharp",
      "vue-demi",
      "workerd"
    ]
  }
}
