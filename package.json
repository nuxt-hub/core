{
  "name": "@nuxthub/core",
  "private": false,
  "version": "1.0.0-alpha.1",
  "description": "Build full-stack Nuxt applications on Cloudflare, with zero configuration.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-hub/core.git"
  },
  "homepage": "https://hub.nuxt.com",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "nuxt",
    "fullstack",
    "cloudflare",
    "database",
    "kv",
    "blob"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "docs": "PORT=4000 nuxi dev docs",
    "docs:build": "nuxi generate docs",
    "release": "npm run lint && npm run test && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --test-timeout=60000",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250810.0",
    "@nuxt/devtools-kit": "^2.4.0",
    "@uploadthing/mime-types": "^0.3.5",
    "db0": "^0.3.2",
    "defu": "^6.1.4",
    "destr": "^2.0.5",
    "h3": "^1.15.4",
    "mime": "^4.0.7",
    "nitro-cloudflare-dev": "^0.2.2",
    "ofetch": "^1.4.1",
    "pathe": "^2.0.3",
    "pkg-types": "^2.1.0",
    "ufo": "^1.6.1",
    "uncrypto": "^0.1.3",
    "unstorage": "^1.16.1",
    "zod": "^3.25.34"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.0",
    "@nuxt/eslint-config": "^1.8.0",
    "@nuxt/kit": "^4.0.3",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "3.19.2",
    "@nuxthub/core": "link:",
    "@types/node": "^22.15.24",
    "changelogen": "^0.6.2",
    "eslint": "^9.33.0",
    "nuxt": "^4.0.3",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "packageManager": "pnpm@10.15.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "@tsparticles/engine",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
