    "@uploadthing/mime-types": "^0.2.7",
    "citty": "^0.1.6",
    "defu": "^6.1.4",
    "h3": "^1.11.1",
    "mime": "^4.0.1",
    "nitro-cloudflare-dev": "^0.1.4",
    "ofetch": "^1.3.4",
    "pathe": "^1.1.2",
    "pkg-types": "^1.0.3",
    "ufo": "^1.5.3",
    "uncrypto": "^0.1.3",
    "unstorage": "^1.10.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.1.5",
    "@nuxt/eslint-config": "^0.3.7",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/test-utils": "^3.12.1",
    "@types/node": "^20.12.7",
    "changelogen": "^0.5.5",
    "eslint": "^9.0.0",
    "nuxt": "^3.11.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0",
    "wrangler": "^3.51.0"
