    "release-it": "^17.0.3",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.27"
  },
  "resolutions": {
    "nitropack": "npm:nitropack-nightly@latest",
    "h3": "npm:h3-nightly@latest"
