{
  "name": "@nuxthub/core",
  "private": false,
  "version": "0.10.0",
  "description": "Build full-stack Nuxt applications, with zero configuration.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-hub/core.git"
  },
  "homepage": "https://hub.nuxt.com",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ]
    }
  },
  "files": [
    "dist",
    "cli"
  ],
  "bin": {
    "nuxthub": "./cli/bin/nuxthub.mjs",
    "nuxt-hub": "./cli/bin/nuxthub.mjs"
  },
  "keywords": [
    "nuxt",
    "fullstack",
    "cloudflare",
    "vercel",
    "database",
    "kv",
    "blob"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "docs": "PORT=4000 nuxi dev docs",
    "docs:build": "nuxi generate docs",
    "release": "npm run lint && npm run test && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --test-timeout=60000",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "nuxthub": "./cli/bin/nuxthub.mjs"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20251127.0",
    "@nuxt/devtools-kit": "^3.1.1",
    "@uploadthing/mime-types": "^0.3.6",
    "chokidar": "^5.0.0",
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "db0": "^0.3.4",
    "defu": "^6.1.4",
    "destr": "^2.0.5",
    "drizzle-kit": "0.31.7",
    "execa": "^9.6.0",
    "get-port-please": "^3.2.0",
    "h3": "^1.15.4",
    "mime": "^4.1.0",
    "nitro-cloudflare-dev": "^0.2.2",
    "nypm": "^0.6.2",
    "ofetch": "^1.5.1",
    "pathe": "^2.0.3",
    "pkg-types": "^2.3.0",
    "std-env": "^3.10.0",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.16.7",
    "ufo": "^1.6.1",
    "uncrypto": "^0.1.3",
    "unstorage": "^1.17.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.11.0",
    "@nuxt/kit": "^4.2.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.1",
    "@nuxt/test-utils": "3.20.1",
    "@nuxthub/core": "link:",
    "@types/node": "^24.10.1",
    "better-sqlite3": "^12.4.6",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.1",
    "jiti": "^2.6.1",
    "nitropack": "^2.12.9",
    "nuxt": "^4.2.1",
    "typescript": "~5.9.3",
    "vitest": "^4.0.14",
    "vue-tsc": "^3.1.5"
  }
}
