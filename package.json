    "release": "release-it"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240129.0",
    "drizzle-orm": "^0.29.3",
    "image-meta": "^0.2.0",
    "mime": "^4.0.1",
    "nuxt-auth-utils": "^0.0.15",
